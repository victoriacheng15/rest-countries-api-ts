{
  "name": "react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "rome:check": "pnpm exec rome check src/",
    "rome:apply": "pnpm exec rome check src/ --apply",
    "rome:format": "pnpm exec rome format src/",
    "rome:write": "pnpm exec rome format src/ --write",
    "prettier:tw": "prettier src/ --plugin=prettier-plugin-tailwindcss --write",
    "format": "pnpm prettier:tw && pnpm rome:write",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-transform-css": "^6.0.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "rome": "^10.0.1",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
